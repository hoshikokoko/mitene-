<div class="container">
  <%= render 'searches/form' %>
  <div class="row">
    <div class="col-2">
      <%= render 'layouts/sidebar_left', tag_lists: @tag_lists %>
    </div>
      <h3>"<%= @content %>"の検索結果</h3>
      <% if @model == 'staff' %>
        <%= render 'admin/staffs/index', staffs: @records %>
      <% else%>
        <%= render 'staff/infomations/index', infomations: @records %>
      <% end %>
    <div class="col-2">
      <%= render 'layouts/sidebar_right' %>
    </div>
  </div>
</div>